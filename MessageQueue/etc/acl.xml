<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="I95DevConnect_MessageQueue::apimethods" title="I95dev Methods" sortOrder="10"/>
                <resource id="Magento_Backend::content">
                    <resource id="I95DevConnect_MessageQueue::blog_content" title="Blog" sortOrder="10">
                        <resource id="I95DevConnect_MessageQueue::post" title="Blog Posts" sortOrder="40">
                            <resource id="I95DevConnect_MessageQueue::save" title="Save Blog Post" sortOrder="10"/>
                            <resource id="I95DevConnect_MessageQueue::post_delete" title="Delete Blog Post"
                                      sortOrder="20"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>