<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <container name="listing_top">
    </container>
    <columns name="sales_order_columns">
        <column name="origin" class="I95DevConnect\Base\Ui\Component\Order\Column\Origin">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">true</item>
                    <item name="editor" xsi:type="string">false</item>
                    <item name="label" xsi:type="string" translate="true">Origin</item>
                    <item name="sortOrder" xsi:type="number">15</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        <column name="target_order_id" class="I95DevConnect\Base\Ui\Component\Order\Column\TargetOrderId">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">true</item>
                    <item name="editor" xsi:type="string">false</item>
                    <item name="label" xsi:type="string" translate="true">Target Order ID</item>
                    <item name="sortOrder" xsi:type="number">15</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>
